<section id="smileys">
	<a name="smileys"></a>
	<div class="bar"><h2>Smileys</h2></div>

	<table>
		<thead>
			<tr>
				<th>#</th>
				<th>Smiley</th>
				<th>Uses</th>
				<th>Last used by</th>
			</tr>

		</thead>
		<tbody>
			<% for( var i=0, count=lines.length; i < count; ++i ) { %>

				<tr class="<%= i%2===0?'odd':'even' %>">
					<td class="pos">
						<%= i+1 %>
					</td>
					<td>
						<%= lines[i].smiley %>
					</td>
					<td>
						<%= lines[i].uses %>
					</td>
					<td>
						<%= lines[i].by %>
					</td>
				</tr>

			<% } %>

		</tbody>

	</table>

	<p>The table describes the most commonly used smileys in the channel.</p>


</section>